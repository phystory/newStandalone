# controlpacket.tcl
# Control variables; 10/25/2005, JPC
# If we want to include BCS on the Readout (activated by default)
set Readout(BCS) 1;# 0=no BCS Readout, 1=BCS Readout (scalers always read)
set Readout(SeGA) 0;# 0=no SeGA Readout, 1=SeGA Readout (scalers always read)
set Readout(NERO) 0;# 0=no NERO Readout, 1=NERO Readout (scalers always read)

